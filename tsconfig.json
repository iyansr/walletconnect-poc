{
   "compilerOptions": {
      "types": [
         "jest",
         "node"
      ],
      "experimentalDecorators": true,
      "jsx": "react-native",
      "module": "commonjs",
      "noEmit": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
         "@modules/*": [
            "./modules/*"
         ]
      }
   },
   "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.ts"
   ]
}